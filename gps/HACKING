rfcomm bind 0 00:0D:B5:38:BA:C6
gpsbabel -i dg-100 -f /dev/rfcomm0 -x track,move=+$(python -c 'import time as t; n = t.time(); print int(t.mktime(t.localtime(n))-t.mktime(t.gmtime(n)))') -o gpx -F $(date +%Y-%m-%d).gpx
gpsbabel -i dg-100,erase=1 -f /dev/rfcomm0 -o gpx -F deleted.gpx
gpsbabel -i gpx -f $(date +%Y-%m-%d).gpx -o kml,units=m -F $(date +%Y-%m-%d).kml
rfcomm release 0
